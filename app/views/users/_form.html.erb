<%= form_for @user do |f| %>
<div>
  <%= f.label :username %>
  <%= f.text_field :username, class: 'txtfield' %>
</div>
<div>
  <%= f.label :first_name %>
  <%= f.text_field :first_name, class: 'txtfield' %>
</div>
<div>
  <%= f.label :last_name %>
  <%= f.text_field :last_name, class: 'txtfield' %>
</div>
<div>
  <%= f.label :email %>
  <%= f.email_field :email, class: 'txtfield' %>
</div>
<div>
  <%= f.label :password %>
  <%= f.password_field :password, class: 'txtfield' %>
</div>
<div>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation, class: 'txtfield' %>
</div>
<div>
  <%= f.label :bio %>
  <%= f.text_area :bio, class: 'txtfield' %>
</div>
<div>
  <%= f.label :avatar %>
  <%= f.file_field :avatar, class: 'txtfield' %>
</div>
<div>
  <%= f.label :street %>
  <%= f.text_field :street, class: 'txtfield' %>
</div>
<div>
  <%= f.label :city %>
  <%= f.text_field :city, class: 'txtfield' %>
</div>
<div>
  <%= f.label :province %>
  <%= f.text_field :province, class: 'txtfield' %>
</div>
<div>
  <center><%= f.submit "Create", class: "btn btn-success" %></center>
</div>
<% end %>