      text-align: center;
      background: url('images/tile2.png');
    }
    section.contact img {
      margin: 20px;
    }
    section.box {
      padding: 50px 100px;
    }
    section.box h2, section.box h3 {
      color: #1A4876;
    }
    .menu {
      list-style: none;
      padding: 0;
      margin: 0;
      text-align: center;
